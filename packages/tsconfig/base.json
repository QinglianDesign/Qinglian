{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "checkJs": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "lib": ["ES2015","DOM"],
    "resolveJsonModule": true
  },
  "rootDit": ".",
  "exclude": ["node_modules"]
}
